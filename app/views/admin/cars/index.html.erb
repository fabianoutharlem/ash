<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Alle autos</h2>
    <ol class="breadcrumb">
      <li>
        <a href="<%= url_for admin_root_path %>">Home</a>
      </li>
      <li>
        <a>Autos</a>
      </li>
      <li class="active">
        <strong>Alle autos</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">

  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>Alle autos (<%= @cars.total_count %>)</h5>
          <div class="ibox-tools">
            <a class="collapse-link">
              <i class="fa fa-chevron-up"></i>
            </a>
          </div>
        </div>
        <div class="ibox-content">

          <%= render partial: 'cars_table', locals: {cars: @cars} %>

          <%= paginate @cars, theme: 'twitter-bootstrap-3', :pagination_class => "pagination-small pagination-centered" %>

        </div>
      </div>
    </div>

  </div>
</div>
